//MSC 30 DAYS CHALLENGE
//DAY 6 MSC 30 DAYS CHALLENGE
#include <bits/stdc++.h>
using namespace std;
int main() {
   int N;
   cin>>N;
   int array[N]; 
   for(int i=0;i<N;i++){
       cin>>array[i];
   }
   for(int i=N-1;i>=0;i--){
       cin>>array[i];
       cout<<array[i]<<" ";
   }
    return 0;
}
