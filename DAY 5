//MSC 30 DAYS CHALLENGE
//DAY 5 MSC 30 DAYS CHALLENGE
#include<iostream>
using namespace std;
int main(){
    int a,b,c,d;
    cin>>a;
    cin>>b;
    cin>>c;
    cin>>d;
    int left_max=max(a,b);
    int right_max=max(c,d);
    int final_max=max(left_max,right_max);
    cout<<final_max<<endl;
    return 0;
}
